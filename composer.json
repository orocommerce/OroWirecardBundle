{
    "name": "oro/commerce-wirecard",
    "description": "OroCommerce Wirecard integration",
    "homepage": "https://github.com/orocommerce/OroWirecardBundle.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocommerce.com"
        }
    ],
    "autoload": {
        "psr-4": {"Oro\\Bundle\\WirecardBundle\\": ""},
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "php": "7.1.*",
        "oro/commerce": "1.6.*",
        "guzzlehttp/guzzle": "^6.0.0",
        "hochstrasserio/wirecard": "^1.0"
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.6.x-dev"
        }
    }
}
